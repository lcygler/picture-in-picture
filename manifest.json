{
  "manifest_version": 3,
  "name": "Picture-in-Picture",
  "version": "1.0",
  "description": "Force Picture-in-Picture mode.",
  "permissions": ["activeTab", "scripting"],
  "background": {
    "service_worker": "background.js"
  },
  "icons": {
    "128": "icon.png"
  },
  "action": {
    "default_icon": {
      "128": "icon.png"
    }
  }
}
